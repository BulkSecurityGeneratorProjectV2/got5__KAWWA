	<script>
	
        function initCapaEsperar(){               	 
    	 	 $j("#capaEsperar").hide();    
        } 
                  
        function callback() {
  			//alert('callback');
  			setTimeout(function() {
  				$j("#header").removeAttr("style").hide().fadeIn();
  			}, 1000 );
  	    }
         
     	function ocultarHeader() {

     			var selectedEffect = 'blind';

     			var options = {};
     			     			
     			$j("#header").hide(3000);
     			$j("#espacioHeader").hide();
     			$j("#ocultarCabeceraLink" ).hide();
     			$j("#mostrarCabeceraLink" ).show('blind',null,3000);     		
     	}
     	 
     	function mostrarHeader() {
     		var selectedEffect = 'blind';

 			var options = {};
 			
 			$j("#mostrarCabeceraLink" ).hide();
  			$j("#header").show(3000);  				  			  			
  			$j("#espacioHeader").show();
  			$j("#ocultarCabeceraLink" ).show();
  			
  	    }
     	
     	function hacerArrastrable() {
     	   $j("#panelErrores").draggable();
     	   $j("#colapsable_buscar").draggable();
     	   $j("#colapsable_captacion").draggable();
     	   $j("#colapsable_datos_personales").draggable();
     	   $j("#colapsable_datos_comerciales").draggable();
     	   $j("#colapsable_datos_pago").draggable();
     	   $j("#botonesDetalleTitular").draggable();
     	  
     	   $j("#header").draggable();
     	}
     	
	</script>
</head>


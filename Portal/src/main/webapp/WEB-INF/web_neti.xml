<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>frontsolutions Tapestry 5 Application</display-name>
	<context-param>
		<param-name>tapestry.app-package</param-name>
		<param-value>awl.frontsolutions</param-value>
	</context-param>
	<context-param>
		<param-name>awl.filesystemindexer.root</param-name>
		<param-value>/WEBS/UniteOD/kawwa/docs/kawwa_components_angela</param-value>
	</context-param>
	<context-param>
        <param-name>tapestry.production-mode</param-name>
        <param-value>true</param-value>
    </context-param>
    <context-param>
        <param-name>kawwa2.friends</param-name>
        <param-value>altima</param-value>
    </context-param>
	<context-param>
        <param-name>ganalytics.key</param-name>
        <param-value>UA-9983102-5</param-value>
    </context-param>
    <context-param>
        <param-name>enableAnalytics</param-name>
        <param-value>true</param-value>
    </context-param>
	<filter>
		<filter-name>app</filter-name>
		<filter-class>org.apache.tapestry5.TapestryFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>app</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
      

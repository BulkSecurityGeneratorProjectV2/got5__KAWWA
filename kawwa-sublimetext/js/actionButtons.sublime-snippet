  <content><![CDATA[function toPrint() {
	if (jQuery("a.bt-print")) {
		jQuery("a.bt-print").click (function(){
			window.print();	
			return false;
		});
	}
}


jQuery(document).ready(function(){
	toPrint();
});]]></content>
